13 Impedance recovery
14 Impedance Recovery},
author = {Withington, S. and Isaak, K. G. and Kovtonyuk, S. A. and Panhuyzen, R. A. and Klapwijk, T. M.},
doi = {10.1016/1350-4495(95)00058-5},
file = {:C$\backslash$:/Users/Jakob/OneDrive - Nexus365/Literature/SIS{\_}Mixers/Direct detection at submillimetre wavelengths using superconducting tunnel junctions.pdf:pdf},
issn = {13504495},
journal = {Infrared Physics and Technology},
keywords = {Bias,Detector Saturation,Embedding Circuit,Impedance Match,Impedance Recovery,Linearity},
mendeley-tags = {Bias,Detector Saturation,Embedding Circuit,Impedance Match,Impedance Recovery,Linearity},
pages = {17},
title = {{Direct detection at submillimetre wavelengths using superconducting tunnel junctions}},
